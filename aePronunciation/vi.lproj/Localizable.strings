// Learn sounds tab
"p_description" = "/p/ là một phụ âm vô thanh. /p/ được bật hơi ([pʰ]) ở đầu từ nhưng nó không được bật hơi ([p]) sau /s/ và ở cuối từ. Nguyên âm có âm ngắn hơn khi chúng đứng trước /p/.";
"t_description" = "/t/ là một phụ âm vô thanh. /t/ là âm bật hơi ([tʰ]) ở đầu từ nhưng nó không được bật hơi ([t]) sau /s/ và ở cuối từ. Hãy cẩn thận, không đọc ví dụ thứ ba là [ɪtə] hoặc [tʰ] hoặc [ɪts]. Cũng lưu ý rằng chữ \"t\" thường thay đổi cách phát âm trong tiếng Anh Mỹ. Xem /ʔ/ và /ɾ/. Nguyên âm có âm ngắn hơn khi chúng đứng trước /t/.";
"k_description" = "/k/ là một phụ âm vô thanh. /k/ được bật âm ([kʰ]) ở đầu từ nhưng nó không được bật âm ([k]) sau /s/ và ở cuối từ. Nguyên âm có âm ngắn hơn khi chúng đứng trước /k/.";
"ch_description" = "/ʧ/ là một phụ âm vô thanh. Lưỡi của bạn bắt đầu từ đỉnh miệng phía sau răng của bạn. Nguyên âm có âm ngắn hơn khi chúng đứng trước /ʧ/.";
"f_description" = "/f/ là một phụ âm vô thanh. Chạm nhẹ vào răng trên của bạn vào bên trong môi dưới của bạn. Nguyên âm có âm ngắn hơn khi chúng đứng trước /f/.";
"th_voiceless_description" = "/θ/ là một phụ âm vô thanh. Trong âm thanh này, lưỡi chạm nhẹ vào đáy của răng trên cùng. Đừng nói /s/ hoặc /t/. Nguyên âm có âm ngắn hơn khi chúng đứng trước /θ/.";
"s_description" = "/s/ là một phụ âm vô thanh. Đặt lưỡi của bạn ở phía trên của miệng của bạn ngay phía sau răng của bạn. Nguyên âm có âm ngắn hơn khi chúng đứng trước /s/.";
"sh_description" = "/ʃ/ là một phụ âm vô thanh. Lưỡi của bạn hơi cong lên đến đỉnh miệng trở lại từ nơi bắt đầu /s/. Nguyên âm có âm ngắn hơn khi chúng đứng trước /ʃ/.";
"b_description" = "/b/ là một phụ âm hữu thanh. Môi và miệng của bạn ở cùng vị trí với âm /p/. Nguyên âm có âm dài hơn khi chúng đứng trước /b/.";
"d_description" = "/d/ là một phụ âm hữu thanh. Môi và miệng của bạn ở cùng vị trí với âm /t/. Hãy cẩn thận, không đọc ví dụ thứ ba là /kɑrdə/ hoặc /kɑrt/. Nguyên âm có âm dài hơn khi chúng đứng trước /d/.";
"g_description" = "/g/ là một phụ âm hữu thanh. Môi và miệng của bạn ở cùng vị trí với âm /k/. Hãy cẩn thận, không đọc ví dụ thứ ba là /pɪgə/ hoặc /pɪk/. Nguyên âm có âm dài hơn khi chúng đứng trước /g/.";
"dzh_description" = "/ʤ/ là một phụ âm hữu thanh. Môi và miệng của bạn ở cùng vị trí với âm /ʧ/. Hãy cẩn thận, không đọc ví dụ thứ ba là /ɛʤə/. Nguyên âm có âm dài hơn khi chúng đứng trước /ʤ/.";
"v_description" = "/v/ là một phụ âm hữu thanh. Lưu ý sự khác biệt giữa /v/ và /w/. Trong âm /v/, răng trên chạm vào môi dưới. Đây là vị trí miệng giống như /f/. Sự khác biệt duy nhất là /v/ là âm hữu thanh. Nguyên âm có âm dài hơn khi chúng đứng trước /v/.";
"th_voiced_description" = "/ð/ là một phụ âm hữu thanh. Lưỡi của bạn ở cùng vị trí với âm /θ/. Lưỡi chạm nhẹ vào đáy răng. Nguyên âm có âm dài hơn khi chúng đứng trước /ð/.";
"z_description" = "/z/ là một phụ âm hữu thanh. Lưỡi của bạn ở cùng vị trí với âm /s/. Nguyên âm có âm dài hơn khi chúng đứng trước /z/.";
"zh_description" = "/ʒ/ là một phụ âm hữu thanh. Nó sử dụng cùng một vị trí miệng như /ʃ/. Nguyên âm có âm dài hơn khi chúng đứng trước /ʒ/.";
"m_description" = "/m/ là một phụ âm giọng mũi. Môi của bạn ở cùng vị trí với /b/, nhưng /m/ cho không khí đi qua mũi của bạn. Nguyên âm có âm dài hơn khi chúng đứng trước /m/.";
"n_description" = "/n/ là một phụ âm giọng mũi. Lưỡi và miệng của bạn ở cùng vị trí với /d/, nhưng /n/ cho không khí đi qua mũi của bạn. Nguyên âm có âm dài hơn khi chúng đứng trước /n/.";
"ng_description" = "/ŋ/ là một phụ âm giọng mũi. Lưỡi của bạn ở cùng vị trí với /g/, nhưng /ŋ/ cho không khí đi qua mũi của bạn. Đừng tạo ra âm /g/ sau nó. /ŋ/ không bắt đầu bất kỳ từ nào trong tiếng Anh. Nguyên âm có âm dài hơn khi chúng đứng trước /ŋ/.";
"l_description" = "/l/ là một phụ âm hữu thanh. Ở đầu của từ, [l] là ký hiệu IPA thích hợp. Tuy nhiên, ở cuối từ, nó được phát âm khác nhau. IPA cho âm thanh này được viết là [ɫ] nhưng chúng tôi sử dụng ký hiệu /l/ cho cả [l] và [ɫ] trong ứng dụng này (cũng như nhiều sách giáo khoa và từ điển khác). Ví dụ 2 cũng có thể được viết là /kuəl/.";
"w_description" = "/w/ là một phụ âm hữu thanh. Lưu ý sự khác biệt giữa /w/ và /v/. Trong âm /w/, môi tạo thành một vòng tròn nhỏ và không chạm vào răng. Một số ít người Mỹ phát âm \"wh\" là /hw/ (trong IPA, đây là /ʍ/). /w/ không bao giờ đứng cuối từ trong tiếng Anh.";
"j_description" = "/j/ là một phụ âm hữu thanh. Lưỡi của bạn bắt đầu gần đầu miệng hơn một chút so với âm /i/. Lưu ý sự khác biệt giữa \"year\" và \"ear\". /j/ không xuất hiện ở cuối bất kỳ từ nào trong tiếng Anh.";
"h_description" = "/h/ là một phụ âm vô thanh. Đó là một âm rất nhẹ mà bạn tạo ra trong cổ họng, không phải ở phía sau miệng. /h/ không xuất hiện ở cuối bất kỳ từ nào trong tiếng Anh.";
"r_description" = "/r/ là một phụ âm hữu thanh. Âm tnày nên được viết là /ɹ/ theo IPA. (IPA /r/ thực sự là một phụ âm rung r.) Tuy nhiên, để duy trì tính nhất quán với nhiều từ điển và sách giáo khoa, ký hiệu /r/ được sử dụng ở đây. Đối với \"r\" ở cuối âm tiết và từ, ứng dụng này sử dụng /ɝ/, /ɚ/, và /r/ (như trong /ɑr/, /ɛr/, /ɪr/, và /ɔr/).";
"glottal_stop_description" = "Âm /ʔ/ được gọi là âm tắc thanh hầu. Nó không phải luôn luôn được dạy trong sách giáo khoa tiếng Anh, nhưng nó là một phần quan trọng của phát âm tiếng Anh Mỹ. Lưu ý sự khác biệt trong cách phát âm của \"put\" ở đây và trong ví dụ cho âm /ʊ/. Âm /ʔ/ thường thay thế cho /t/, đặc biệt là ở cuối từ và ở giữa câu.";
"flap_t_description" = "Âm /ɾ/ (không phải /r/) được gọi là âm vỗ t hoặc âm lợi. Nó thường không được dạy trong sách giáo khoa tiếng Anh, nhưng nó là một phần quan trọng của phát âm tiếng Anh Mỹ. Âm /ɾ/ thường thay thế /t/, đặc biệt là khi /t/ xuất hiện giữa các nguyên âm trong một âm tiết không phải là trọng âm. Âm /ɾ/ cũng có thể được viết /d/, mặc dù điều này không chính xác. Trong trường hợp đó, các ví dụ sẽ được viết là /\'wɑdɚ/, /\'lɪdəl/, ɑnd /\'θɝdi/.";

"i_description" = "Tạo âm /i/ với lưỡi cao trong vòm miệng và ngay sau răng cửa. Trong phát âm tiếng Anh của người Anh, âm này thường được viết là /iː/, trong đó hai dấu chấm đại diện cho một nguyên âm dài. Nguyên âm dài và ngắn không được phân biệt nhiều bằng tiếng Anh Mỹ và do đó hai dấu chấm không được viết. Nếu một nguyên âm đứng trước một phụ âm hữu thanh thì nó dài. Nếu nó là trước một phụ âm vô thanh thì nó ngắn.";
"i_short_description" = "/ɪ/ là một âm thanh khác với /i/. Sự khác biệt không phải là dài hay ngắn, mà là ở một vị trí khác nhau của miệng và lưỡi. Miệng mở rộng hơn một chút và lưỡi chỉ lùi lại một chút và thấp hơn /i/.";
"e_short_description" = "Nhiều từ điển và sách giáo khoa mô tả tiếng Anh Anh sử dụng ký hiệu /e/ cho âm này. Theo IPA, mặc dù vậy, nó nên được viết là /ɛ/. Miệng mở ít hơn với /æ/ và lưỡi hơi hướng về phía trước một chút.";
"ae_description" = "Kéo dài môi của bạn như /ɛ/ nhưng mở to miệng như /ɑ/. Lưỡi của bạn nên lùi xa hơn so với /ɛ/.";
"a_description" = "Mở rộng miệng và thư giãn đôi môi của bạn. Đặt lưỡi của bạn thấp và lùi xa trở lại trong miệng của bạn. /ɑ/ cũng được viết là /ɑː/ trong tiếng Anh Anh. Tuy nhiên, trong tiếng Anh Mỹ, đôi khi /ɑ/ là dài (trước phụ âm hữu thanh) và đôi khi nó ngắn (trước phụ âm không vô thanh). Lưu ý rằng đây là một âm rất khác so với /ʌ/ trong tiếng Anh Mỹ.";
"c_backwards_description" = "/ɔ/ nên được viết là /ɒ/ theo IPA. Tuy nhiên, để phù hợp với hầu hết các từ điển, /ɔ/ ​​được sử dụng ở đây. Tuy nhiên, lưu ý rằng đó không phải là âm giống như khi /ɔː/ được sử dụng để mô tả phát âm tiếng Anh. Nó cũng không giống với âm ɔ trong /ɔɪ/ hoặc /ɔr/ (Trong Tiếng Anh Anh /ɔː/ giống với âm ɔ trong /ɔɪ/ hoặc /ɔr/). Khoảng 40% người Mỹ không phát âm âm /ɔ/. Họ thay thế nó bằng âm /ɑ/. Các ví dụ đã cho sẽ được phát âm là /ɑt/, /kɑl/, ɑnd /sɑ/. Các từ \"cot\" và \"caught\" cũng sẽ được phát âm giống nhau (/kɑt/).";
"u_short_description" = "Để nói /ʊ/, hãy làm cho đôi môi của bạn tròn, to xung quanh như thể bạn đang mút ngón tay cái của bạn. Đôi khi /ʊ/ được viết không chính xác là /u/, nhưng nó nên được viết là /ʊ/ (không có \"đuôi\"). /u/ và /ʊ/ là hai nguyên âm khác nhau.";
"u_description" = "/u/ cũng được viết là /uː/ trong tiếng Anh Anh, nhưng giống như /iː/, /ɑː/, và /ɔː/, các dấu nguyên âm dài không cần thiết trong tiếng Anh Mỹ. /u/ là dài trước phụ âm hữu thanh và ngắn trước phụ âm vô thanh.";
"v_upsidedown_description" = "Tạo âm /ʌ/ với miệng mở rộng khoảng một ngón tay. Tuy nhiên, môi và lưỡi của bạn nên được thư giãn. Đây là một âm thanh khác với /ɑ/. Sự khác biệt không phải là dài và ngắn, mà là ở một vị trí khác nhau của miệng và lưỡi. Lưu ý rằng nó cũng được phát âm hơi khác so với âm Anh thường được viết là /ʌ/. Khi âm này nằm trong một âm tiết không là trọng âm, nó được viết là /ə/.";
"schwa_description" = "Nguyên âm /ə/ là âm phổ biến nhất trong tiếng Anh Mỹ vì được sử dụng trong các âm tiết và từ không nhấn trọng âm. Nó được gọi là schwa. Khi âm này nằm trong một âm tiết là trọng âm, nó được viết là /ʌ/.";
"ei_description" = "/eɪ/ là một nguyên âm đôi, là sự kết hợp của âm /e/ và /ɪ/. Nó đôi khi cũng được viết là /e/ trong một số từ điển tiếng Anh Mỹ. (Đừng nhầm lẫn điều này với ký hiệu tiếng Anh Anh /e/, nó thực ra được phát âm là /ɛ/.) Các từ có tiếng Anh Mỹ /eɪ/ được phát âm là /ɛɪ/ trong tiếng Anh Anh. Tuy nhiên, họ vẫn viết nó là /eɪ/.";
"ai_description" = "/aɪ/ là một nguyên âm đôi, sự kết hợp của âm /a/ và /ɪ/. Lưu ý rằng /a/ không hoàn toàn giống với âm /ɑ/.";
"au_description" = "/aʊ/ là một nguyên âm đôi, là sự kết hợp của âm /a/ và /ʊ/. Lưu ý cách miệng bắt đầu to và rộng vàn dần nhỏ và tròn.";
"oi_description" = "/ɔɪ/ là một nguyên âm đôi, sự kết hợp của các âm /ɔ/ và /ɪ/. \"ɔ\" được sử dụng trong /ɔɪ/ không giống với âm /ɔ/ trong ứng dụng này. (Điều này là do âm /ɔ/ được sử dụng để mô tả nhiều từ tiếng Anh Mỹ nên thực ra được viết là /ɒ/.) Âm /ɔɪ/ bắt đầu bằng cách đưa môi tròn nhỏ.";
"ou_description" = "/oʊ/ là một nguyên âm đôi, là sự kết hợp của các âm /o/ và /ʊ/. Một số từ điển chỉ viết nó là /o/, và trên thực tế, nó sẽ chính xác hơn khi sử dụng /o/ chứ không phải là nguyên âm đôi cho một số từ. Lưu ý rằng có một chút khác biệt so với cách phát âm tiếng Anh Anh của /əʊ/.";
"er_stressed_description" = "Âm /ɝ/ đôi khi được viết là /r/ hoặc /əɹ/. Đây là âm trong một âm tiết được nhấn trọng âm hoặc một từ một âm tiết. Khi /ɝ/ không được nhấn trọng âm, nó được viết là /ɚ/. Trong tiếng Anh Anh /ɝ/ được phát âm là  /ɜː/.";
"er_unstressed_description" = "Âm /ɚ/ giống như /ɝ/ ngoại trừ âm /ɚ/ được sử dụng cho các âm tiết không nhấn trọng âm. Đôi khi nó cũng được viết là /ər/ hoặc /əɹ/. Trong tiếng Anh Anh /ɚ/ được phát âm là /ə/.";
"ar_description" = "Âm /ɑr/ được viết chính xác hơn là /ɑɹ/ hoặc /ɑɚ/ theo IPA. Nó được phát âm là /ɑː/ trong tiếng Anh Anh.";
"er_description" = "Âm /ɛr/ được viết chính xác hơn là /ɛɹ/ hoặc /ɛɚ/ theo IPA. Nó được phát âm và viết là /eə/ trong tiếng Anh Anh.";
"ir_description" = "TÂm /ɪr/ được viết chính xác hơn là /ɪɹ/ hoặc /ɪɚ/ theo IPA. Nó được phát âm là /ɪə/ trong tiếng Anh Anh.";
"or_description" = "\"ɔ\" được sử dụng trong /ɔr/ không giống với âm /ɔ/ trong ứng dụng này. (Điều này là do âm /ɔ/ được sử dụng để mô tả nhiều từ tiếng Anh Mỹ nên được viết là /ɒ/.) Âm /ɔr/ phát âm bắt đầu bằng đôi môi tròn nhỏ. Nó được viết chính xác hơn là /ɔɹ/ hoặc /ɔɚ/ theo IPA. Nó được phát âm là /ɔː/ trong tiếng Anh Anh.";
